{
  "name": "dev-dot",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "prebuild:deploy": "node-sass _sass/style.scss css/style.css",
    "build:deploy": "postcss -u autoprefixer -o css/style.css css/style.css",
    "build": "NODE_ENV=production npm run build:deploy && npm run build-js-bundle && npm run build-get-started-bundle && npm run build-all-pages",
    "prebuild:dev": "node-sass --recursive --source-map=true --source-map-contents=true --source-map-embed=true _sass/style.scss css/style.css",
    "build:dev": "postcss -u autoprefixer -o css/style.css css/style.css",
    "post-dev": "watch 'npm run build:dev' _sass | npm run watch-js-bundle | npm run watch-get-started-bundle | jekyll serve",
    "dev": "npm run build-all-pages && npm run post-dev",
    "watch-js-bundle": "mkdir -p dynamic/public/javascript/build && watchify -t babelify --extension=.jsx --extension=.js -vd dynamic/react/api-app/index.js --outfile dynamic/public/javascript/build/api-bundle.js",
    "build-js-bundle": "mkdir -p dynamic/public/javascript/build && browserify -t envify -t babelify --extension=.jsx --extension=.js -vd dynamic/react/api-app/index.js --outfile dynamic/public/javascript/build/api-bundle.js",
    "build-all-pages": "browserify -t envify -t babelify --node --extension=.jsx --extension=.js -vd dynamic/build-all-pages.js --outfile /tmp/build-all-pages.js && node /tmp/build-all-pages.js",
    "build-page": "browserify -t envify -t babelify --node --extension=.jsx --extension=.js -vd dynamic/build-static-page.js --outfile /tmp/build-static-page.js && node /tmp/build-static-page.js",
    "watch-get-started-bundle": "mkdir -p dynamic/public/javascript/build && watchify -t babelify --extension=.jsx --extension=.js -vd dynamic/react/get-started/index.js --outfile dynamic/public/javascript/build/get-started-bundle.js",
    "build-get-started-bundle": "mkdir -p dynamic/public/javascript/build && browserify -t envify -t babelify --extension=.jsx --extension=.js -vd dynamic/react/get-started/index.js --outfile dynamic/public/javascript/build/get-started-bundle.js",
    "eslint": "eslint --ext .js,.jsx ./dynamic"
  },
  "devDependencies": {
    "autoprefixer": "^6.3.7",
    "aws-sdk": "^2.4.8",
    "babel-eslint": "^6.1.2",
    "babel-plugin-transform-object-rest-spread": "^6.8.0",
    "babel-plugin-transform-strict-mode": "^6.11.3",
    "babel-preset-es2015": "^6.1.18",
    "babel-preset-react": "^6.11.1",
    "babelify": "^7.2.0",
    "browserify": "^13.0.1",
    "envify": "^3.4.1",
    "eslint": "^3.1.1",
    "eslint-plugin-react": "^5.2.2",
    "mkdirp": "^0.5.1",
    "node-sass": "^3.8.0",
    "postcss-cli": "^2.5.2",
    "ramda": "^0.21.0",
    "react": "^15.2.1",
    "react-dom": "^15.2.1",
    "react-markdown": "^2.4.2",
    "react-redux": "^4.4.5",
    "redux": "^3.5.2",
    "redux-logger": "^2.6.1",
    "request": "^2.73.0",
    "swagger-parser": "^3.4.1",
    "watch": "^0.19.1",
    "watchify": "^3.6.1"
  }
}
