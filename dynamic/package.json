{
  "name": "app",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "watchify -t babelify --extension=.jsx -vd react/console/index.js --outfile public/javascript/build/console.js",
    "start-static": "watchify -t babelify --extension=.jsx -vd react/console/index-static.js --outfile public/javascript/build/console-static.js",
    "build-static-page": "browserify -t babelify --node --extension=.jsx -vd ./build-static-page.js --outfile /tmp/build-static-page.js && node /tmp/build-static-page.js",
    "build-all-pages": "browserify -t babelify --node --extension=.jsx -vd ./build-all-pages.js --outfile /tmp/build-all-pages.js && node /tmp/build-all-pages.js",
    "eslint": "eslint --ext .js,.jsx ."
  },
  "devDependencies": {
    "aws-sdk": "^2.4.8",
    "babel-eslint": "^6.1.2",
    "babel-plugin-transform-object-rest-spread": "^6.8.0",
    "babel-plugin-transform-strict-mode": "^6.11.3",
    "babel-preset-es2015": "^6.1.18",
    "babel-preset-react": "^6.11.1",
    "babelify": "^7.2.0",
    "browserify": "^13.0.1",
    "eslint": "^3.1.1",
    "eslint-plugin-react": "^5.2.2",
    "mkdirp": "^0.5.1",
    "ramda": "^0.21.0",
    "react": "^15.2.1",
    "react-dom": "^15.2.1",
    "react-markdown": "^2.4.2",
    "redux": "^3.5.2",
    "request": "^2.73.0",
    "swagger-parser": "^3.4.1",
    "watchify": "^3.6.1"
  }
}
